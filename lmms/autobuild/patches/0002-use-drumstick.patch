-        Alg_event *new_event;
-        if (s[i]->is_note()) {
-            new_event = new Alg_note((Alg_note_ptr) s[i]);
-        } else {
-            new_event = new Alg_update((Alg_update_ptr) s[i]);
-        }
-        new_event->time += t;
-        insert(new_event);
-    }
