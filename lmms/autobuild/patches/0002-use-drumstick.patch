+{
+	midiReader mr(tc);
+	mr.read(filename);
+	return true;
+}
+
+bool SmfImport::readRIFF( TrackContainer* tc )
+{
+	// skip file length
+	skip( 4 );
+
+	// check file type ("RMID" = RIFF MIDI)
+	if( readID() != makeID( 'R', 'M', 'I', 'D' ) )
+	{
+invalid_format:
+			qWarning( "MidiImport::readRIFF(): invalid file format" );
+			return false;
+	}
+
+	// search for "data" chunk
+	while( 1 )
+	{
+		const int id = readID();
+		const int len = read32LE();
+		if( file().atEnd() )
+		{
+data_not_found:
+				qWarning( "MidiImport::readRIFF(): data chunk not found" );
+				return false;
+		}
+		if( id == makeID( 'd', 'a', 't', 'a' ) )
+		{
+				break;
+		}
+		if( len < 0 )
+		{
+				goto data_not_found;
+		}
+		skip( ( len + 1 ) & ~1 );
+	}
+
+	// the "data" chunk must contain data in SMF format
+	if( readID() != makeID( 'M', 'T', 'h', 'd' ) )
+	{
+		goto invalid_format;
+	}
+	return readSMF( tc );
+}
