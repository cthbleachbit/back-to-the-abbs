-int Alg_reader::find_real_in(string &field, int n)
-{
-    // scans from offset n to the end of a real constant
-    bool decimal = false;
-    int len = field.length();
-    for (int i = n; i < len; i++) {
-        char c = field[i];
-        if (!isdigit(c)) {
-            if (c == '.' && !decimal) {
-                decimal = true;
-            } else {
-                return i;
-            }
-        }
-    }
-    return field.length();
-}
